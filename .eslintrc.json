{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "consistent-return": [0],
    "arrow-body-style": [1, "as-needed"],
    "import/no-unresolved": [0],
    "max-len": [1, { "code": 175 }],
    "strict": [0, "global"],
    "no-plusplus": [0],
    "no-restricted-syntax": "off"
  }
}
